#register{
    border: 1px solid #E5E4E4;
    margin-top: 20px;
    width: 296px;
    border-radius:8px;
}
#register .inp_comm{
    float: right;
    width: 245px;
    border: none;
    padding-left: 13px;
    width: 234px;
    outline: none;
  /*  line-height: 44px;*/
    height: 44px;
}
#register hr{
    margin-top: 10px;
    margin-bottom: 0px;
    border: 0;
    height: 27px;
    float: left;
    position: relative;
    margin-left: 10px;
    left: 24px;
    background-color: #E5E4E4;
}
#register .inp_comm:focus{
    border: none;
}
#register .inp_comm_yanm{
    border: none;
    width: 138px;
    border-right: 1px solid #E5E4E4;
    outline: none;
    line-height: normal;
}
#register .row{
    border-bottom: 1px solid #E5E4E4;
}
#register .row:last-child{
    border-bottom: none;
}
#register .row{
    height: 48px;
    line-height: 48px;
}
#register .get_yanm{
  /*  margin-left: 25px; */
float: right;
    height: 36px;
    display: inline-block;
    /* padding: 5px 0px; */
    margin-bottom: 0;
    font-size: 10px;
    font-weight: 400;
    line-height: 0.428571;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    -o-user-select: none;
    user-select: none;
    /* width: 100%; */
    color: white;
    background-color: #3AD5B8;
    letter-spacing: 3px;
    font-weight: bold;
    margin-top: 5px;
}
.btn_qued{
    display: inline-block;
    padding: 8px 12px;
    margin-bottom: 0;
    font-size: 14px;
    font-weight: 400;
    line-height: 1.428571429;
    text-align: center;
    white-space: nowrap;
    vertical-align: middle;
    cursor: pointer;
    background-image: none;
    border: 1px solid transparent;
    border-radius: 4px;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    -o-user-select: none;
    user-select: none;
    width: 100%;
    color: white;
    background-color: #3AD5B8;
    letter-spacing: 6px;
    font-weight: bold;
    margin-top: 20px;
}
